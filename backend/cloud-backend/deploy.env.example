# Required
PROJECT_ID=your-project-id
REGION=asia-south1
STORAGE_BUCKET=medgemini-tb-triage.firebasestorage.app

# Model versioning
TARGET_MODEL_VERSION=medgemma-4b-it-v1
MODEL_VERSION=medgemma-4b-it-v1

# Names
AR_REPO=tb-backend
RUN_SERVICE=tb-inference
QUEUE_NAME=tb-inference-queue

# Cloud Run sizing (real MedGemma + HEAR typically needs higher headroom)
INFERENCE_CPU=8
INFERENCE_MEMORY=32Gi
INFERENCE_MAX_INSTANCES=1
INFERENCE_TIMEOUT=3600
USE_GPU=1
GPU_COUNT=1
GPU_TYPE=nvidia-l4
GPU_ZONAL_REDUNDANCY=0

# Service accounts (created by script if missing)
SA_FN_ENQUEUE=sa-fn-enqueue
SA_TASKS_INVOKER=sa-tasks-invoker
SA_INFERENCE=sa-inference

# Optional model artifact sync on startup
SYNC_MODELS_ON_STARTUP=1
WARM_LOAD_ON_STARTUP=0
GCS_MODEL_BUCKET=medgemini-tb-triage.firebasestorage.app
GCS_MEDGEMMA_PREFIX=models/medgemma
GCS_CLASSICAL_PREFIX=models/classical
GCS_HEAR_PREFIX=models/Hear_model/hear_model_offline

# In-container model paths
LOCAL_MEDGEMMA=/models/medgemma
LOCAL_CLASSICAL=/models/classical
LOCAL_HEAR=/models/hear
